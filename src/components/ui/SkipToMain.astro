---
import type { HTMLAttributes } from 'astro/types';

interface Props extends HTMLAttributes<'a'> {
};

const { class: className, ...attributes }: Partial<Props> = Astro.props;
---

<a class="sr-only focus:not-sr-only border-b-2 border-transparent focus:p-2 focus:bg-pink-300 focus:text-black focus:border-b-2 focus:border-black" href="#main" {...attributes} >
  Skip to main content
</a>
